{
  "name": "ng-hub-ui-sortable",
  "version": "20.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build:app": "ng build ng-hub-ui-sortable-app --configuration production --base-href /ng-hub-ui-sortable/ && node scripts/copy-index.js",
    "build:ssr": "ng build ng-hub-ui-sortable-app --configuration production --base-href /ng-hub-ui-sortable/ && node scripts/copy-index.js",
    "build:lib": "ng build ng-hub-ui-sortable",
    "test:app": "ng test ng-hub-ui-sortable-app",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "gh-pages": "git reset && cpr dist/ng-hub-ui-sortable-app docs -d  && cpr docs/index.html docs/404.html && git add docs",
    "gh-pages:apply": "yarn run gh-pages && git commit -m 'docs: update github pages' && git push",
    "release": "wpm release --release-branch-format release-v{{version}}",
    "publish:npm": "npm run build:lib && cd dist/ng-hub-ui-sortable && npm publish --access=public",
    "pub-only": "cd dist/ng-hub-ui-sortable && npm publish --access=public",
    "pub": "wpm publish && npm run pub-only"
  },
  "private": false,
  "description": "Angular bindings for Sortable.js (ng-hub-ui fork of @worktile/ngx-sortablejs)",
  "author": "Carlos Morcillo Fern√°ndez <https://www.carlosmorcillo.com/>",
  "keywords": [
    "ng-hub-ui",
    "sortable",
    "drag-and-drop",
    "angular",
    "sortablejs"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/carlos-morcillo/ng-hub-ui-sortable"
  },
  "bugs": {
    "url": "https://github.com/carlos-morcillo/ng-hub-ui-sortable/issues"
  },
  "homepage": "https://github.com/carlos-morcillo/ng-hub-ui-sortable#readme",
  "dependencies": {
    "@angular/animations": "20.2.4",
    "@angular/common": "20.2.4",
    "@angular/compiler": "20.2.4",
    "@angular/core": "20.2.4",
    "@angular/forms": "20.2.4",
    "@angular/platform-browser": "20.2.4",
    "@angular/platform-browser-dynamic": "20.2.4",
    "@angular/platform-server": "^20.2.4",
    "@angular/router": "20.2.4",
    "@angular/ssr": "^20.3.13",
    "bootstrap": "^5.3.3",
    "bootstrap-icons": "^1.13.1",
    "highlight.js": "^11.11.1",
    "ngx-bootstrap": "^20.0.1",
    "rxjs": "^7.4.0",
    "sortablejs": "^1.10.2",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "20.2.2",
    "@angular-eslint/builder": "20.2.0",
    "@angular-eslint/eslint-plugin": "20.2.0",
    "@angular-eslint/eslint-plugin-template": "20.2.0",
    "@angular-eslint/schematics": "20.2.0",
    "@angular-eslint/template-parser": "20.2.0",
    "@angular/cli": "20.2.2",
    "@angular/compiler-cli": "20.2.4",
    "@angular/language-service": "20.2.4",
    "@semantic-release/changelog": "^3.0.6",
    "@semantic-release/git": "^7.0.18",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^24.3.1",
    "@types/sortablejs": "^1.10.2",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@worktile/pkg-manager": "^0.0.11",
    "codelyzer": "^6.0.0",
    "coveralls": "^3.0.9",
    "cpr": "^3.0.1",
    "eslint": "^8.57.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "^2.0.6",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^20.2.0",
    "ngx-spec": "^2.1.4",
    "protractor": "~7.0.0",
    "semantic-release": "^15.14.0",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.3",
    "typescript": "~5.9.2"
  }
}
